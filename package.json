	"scripts": {
		"clean": "rm -rf node_modules",
		"prebuild": "rm -rf public/dist/*",
		"build": "webpack --progress --colors --config webpack.prod.config.js",
		"start": "node --harmony server/",
		"start:dev": "nodemon --config .nodemon.json",
		"start:dev-debug": "nodemon --config .nodemon.json debug",
		"win:start:prod": "set NODE_ENV=production npm run prebuild && npm run build && node --harmony server/",
		"start:prod": "npm run prebuild && npm run build && NODE_ENV=production node --harmony server/",
		"db:migrate": "NODE_ENV=production sequelize db:migrate",
		"db:migrate:undo": "sequelize db:migrate:undo",
		"db:migrate:undo:all": "sequelize db:migrate:undo:all",
		"test": "npm run jshint && npm run mocha",
		"jshint": "jshint server/. test/. --config",
		"mocha": "mocha test/ --recursive"
	},
